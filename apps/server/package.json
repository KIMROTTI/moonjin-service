{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "typia:init": "yarn typia setup --manager yarn",
    "prebuild": "prisma generate",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "ts-patch install && typia patch",
    "swagger": "npx nestia swagger",
    "sdk:build": "npx nestia sdk && npx tsc -p tsconfig.api.json",
    "sdk:publish": "cd ./api-sdk && npm publish --access=public",
    "pm2:deploy": "pm2 start dist/src/main.js --name=api_server"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.667.0",
    "@aws-sdk/s3-request-presigner": "^3.667.0",
    "@moonjin/editorjs": "workspace:^",
    "@moonjin/editorjs-types": "workspace:^",
    "@nestia/core": "^2.4.3",
    "@nestia/e2e": "^0.4.0",
    "@nestjs/axios": "^3.0.3",
    "@nestjs/common": "^10.4.4",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.4",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.4.4",
    "@nestjs/swagger": "^7.4.2",
    "@prisma/client": "^5.15.0",
    "axios": "^1.7.7",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.7",
    "crypto": "^1.0.1",
    "editorjs-html": "^3.4.3",
    "entity": "*",
    "form-data": "^4.0.0",
    "mailgun.js": "^9.4.0",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "prisma": "^5.15.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typia": "^5.3.7",
    "uuid": "^9.0.1",
    "webpack": "^5.95.0"
  },
  "devDependencies": {
    "@nestia/sdk": "^2.4.3",
    "@nestjs/cli": "^10.4.5",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.4",
    "@types/bcrypt": "^5",
    "@types/bcryptjs": "^2",
    "@types/cookie-parser": "^1.4.7",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/node": "^20.3.1",
    "@types/passport": "^0",
    "@types/passport-jwt": "^4",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-custom": "*",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "nestia": "^5.1.2",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.2.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}